    "@loopback/rest": "^14.0.6",
    "@loopback/rest-explorer": "^7.0.6",
    "@loopback/service-proxy": "^7.0.6",
    "@loopx/core": "workspace:^",
    "@loopx/multi-tenancy": "workspace:^",
    "@loopx/user-core": "workspace:^",
    "@sphericsio/password-sheriff": "^1.1.1",
    "@types/i18n": "^0.13.12",
    "base-64": "^1.0.0",
    "bcrypt": "^5.1.1",
    "berrors": "^0.3.1",
    "body-parser": "^1.20.3",
    "cookie": "^0.6.0",
    "cookie-parser": "^1.4.6",
    "ejs": "^3.1.10",
    "https-proxy-agent": "^7.0.5",
    "jose": "^5.9.3",
    "jsonwebtoken": "^9.0.2",
