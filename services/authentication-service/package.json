{
  "name": "@loopx/authentication-service",
  "version": "0.8.7",
  "description": "The authentication service",
  "keywords": [
    "loopback-extension",
    "loopback"
  ],
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "lb-tsc && yarn run openapi-spec && yarn run apidocs",
    "build:watch": "lb-tsc --watch",
    "openapi-spec": "node ./dist/openapi-spec",
    "apidocs": "widdershins --search false --language_tabs 'javascript:JavaScript:request' 'javascript--nodejs:Node.JS' --summary openapi.json -o openapi.md",
    "pretest": "yarn run clean && yarn run build",
    "test": "yarn pretest && jest",
    "coverage": "nyc yarn test",
    "clean": "lb-clean 'dist/*' '*.tsbuildinfo' '.eslintcache'",
    "test:ci": "jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/betaly/loopx.git",
    "directory": "services/authentication-service"
  },
  "author": "",
  "license": "",
  "files": [
    "README.md",
    "dist",
    "src",
    "migration.js",
    "migrations",
    "database.json",
    "!*/__tests__"
  ],
  "dependencies": {
    "@autha/api": "^0.2.2",
    "@bleco/aliaser": "^1.1.0",
    "@bleco/authentication": "^1.4.7",
    "@bleco/soft-delete": "^0.7.5",
    "@loopback/context": "^6.1.3",
    "@loopback/core": "^5.1.3",
    "@loopback/repository": "^6.1.3",
    "@loopback/rest": "^13.1.3",
    "@loopback/rest-explorer": "^6.1.3",
    "@loopback/service-proxy": "^6.1.3",
    "@loopx/core": "workspace:^",
    "@loopx/multi-tenancy": "workspace:^",
    "@loopx/user-core": "workspace:^",
    "@sphericsio/password-sheriff": "^1.1.1",
    "@types/i18n": "^0.13.8",
    "base-64": "^1.0.0",
    "bcrypt": "^5.1.1",
    "berrors": "^0.3.1",
    "body-parser": "^1.20.2",
    "cookie-parser": "^1.4.6",
    "https-proxy-agent": "^7.0.2",
    "jose": "^4.15.4",
    "jsonwebtoken": "^9.0.2",
    "libphonenumber-js": "^1.10.48",
    "lodash": "^4.17.21",
    "loopback4-acl": "^0.3.7",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.43",
    "nanoid": "^3.3.5",
    "node-fetch": "^2.6.6",
    "nyc": "^15.1.0",
    "otp-generator": "^4.0.1",
    "otplib": "^12.0.1",
    "passport-apple": "^2.0.2",
    "passport-azure-ad": "^4.3.5",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-instagram": "^1.0.0",
    "prom-client": "^15.0.0",
    "qrcode": "^1.5.3",
    "sinon": "^17.0.0",
    "tily": "^2.22.5",
    "tslib": "^2.6.2",
    "uniqid": "^5.4.0"
  },
  "devDependencies": {
    "@loopback/build": "^10.1.3",
    "@loopback/eslint-config": "^14.0.4",
    "@loopback/testlab": "^6.1.3",
    "@types/base-64": "^1.0.1",
    "@types/bcrypt": "^5.0.1",
    "@types/client-sessions": "^0.8.5",
    "@types/cookie-parser": "^1.4.5",
    "@types/google-libphonenumber": "^7.4.29",
    "@types/jsonwebtoken": "^9.0.4",
    "@types/lodash": "^4.14.200",
    "@types/moment-timezone": "^0.5.13",
    "@types/node": "^20.8.7",
    "@types/node-fetch": "^2.5.12",
    "@types/otp-generator": "^4.0.1",
    "@types/passport-apple": "^2.0.2",
    "@types/passport-azure-ad": "^4.3.3",
    "@types/passport-facebook": "^3.0.2",
    "@types/passport-google-oauth20": "^2.0.13",
    "@types/passport-instagram": "^1.0.4",
    "@types/qrcode": "^1.5.4",
    "@types/sinon": "^10.0.20",
    "@types/uniqid": "^5.3.3",
    "client-sessions": "^0.8.0",
    "jest": "^29.7.0",
    "loopback4-plus": "^0.1.2",
    "nock": "^13.3.6",
    "source-map-support": "^0.5.21",
    "typescript": "~5.2.2",
    "widdershins": "^4.0.1"
  }
}
