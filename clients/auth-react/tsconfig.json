{
  "$schema": "http://json.schemastore.org/tsconfig",
  "extends": "@nutol/build/config/tsconfig.common.json",
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "noEmit": true,
    "jsx": "react",
    "incremental": false,
    "outDir": "dist",
    "rootDir": "src"
  },
  "exclude": [
    "node_modules"
  ],
  "include": [
    "src"
  ]
}
