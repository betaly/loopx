{
  "name": "@loopx/auth-js",
  "description": "LoopX authentication for JavaScript written in TypeScript",
  "version": "0.1.0",
  "keywords": [],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "types": "./dist/index.d.ts",
    "require": "./dist/index.cjs",
    "import": "./dist/index.js"
  },
  "engines": {
    "node": ">=16"
  },
  "author": "ty",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "version": "replace '\\d+\\.\\d+\\.\\d+' \\'$(npx json -f package.json version)\\' src/version.ts && git add src/version.ts",
    "build": "nb-tsc && rollup -c",
    "clean": "del 'loopx-auth-js*.tgz' 'dist/*' '*.tsbuildinfo' 'package'",
    "pretest": "yarn run clean && yarn run build",
    "test": "yarn run pretest && yarn run jest",
    "jest": "jest"
  },
  "dependencies": {
    "ts-essentials": "^9.3.2",
    "tslib": "^2.6.1"
  },
  "devDependencies": {
    "@nutol/build": "^0.2.14",
    "@nutol/eslint-config": "^0.2.9",
    "@types/jest": "^29.5.3",
    "@types/node": "^20.4.8",
    "browser-tabs-lock": "^1.3.0",
    "del-cli": "^5.0.0",
    "jest": "^29.6.2",
    "jest-environment-jsdom": "^29.6.2",
    "jest-fetch-mock": "^3.0.3",
    "rollup": "^3.27.2"
  },
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ]
}
