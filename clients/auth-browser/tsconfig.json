{
  "$schema": "http://json.schemastore.org/tsconfig",
  "extends": "@nutol/build/config/tsconfig.common.json",
  "compilerOptions": {
    "target": "esnext",
    "lib": [
      "esnext",
      "dom"
    ],
    "types": [
      "node",
      "jest"
    ],
    "incremental": false,
    "module": "esnext",
    "outDir": "dist",
    "rootDir": "src"
  },
  "exclude": [
    "node_modules"
  ],
  "include": [
    "src"
  ]
}
