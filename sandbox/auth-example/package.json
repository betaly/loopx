{
  "name": "@loopx/auth-example",
  "description": "A sample application for sandbox testing of auth microservice components.",
  "version": "0.2.0",
  "keywords": [],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "engines": {
    "node": ">=16"
  },
  "author": "TY",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "version": "replace '\\d+\\.\\d+\\.\\d+' \\'$(npx json -f package.json version)\\' src/version.ts && git add src/version.ts",
    "build": "nb-tsc -b",
    "clean": "del 'loopx-auth-example*.tgz' 'dist/*' '*.tsbuildinfo' 'package'",
    "pretest": "yarn run clean && yarn run build",
    "test": "yarn run pretest && yarn run jest",
    "jest": "jest",
    "openapi-spec": "yarn build && node ./dist/openapi-spec",
    "apidocs": "widdershins --search false --language_tabs 'javascript:JavaScript:request' 'javascript--nodejs:Node.JS' --summary src/openapi.json -o openapi.md",
    "start": "node -r source-map-support/register .",
    "start:dev": "tsx src/index.ts"
  },
  "dependencies": {
    "@bleco/ratelimiter": "^1.0.0-alpha.1",
    "@loopback/boot": "^6.1.1",
    "@loopback/context": "^6.1.1",
    "@loopback/core": "^5.1.1",
    "@loopback/openapi-v3": "^9.1.1",
    "@loopback/repository": "^6.1.1",
    "@loopback/rest": "^13.1.1",
    "@loopback/rest-explorer": "^6.1.1",
    "@loopback/service-proxy": "^6.1.1",
    "@loopx/authentication-service": "workspace:^",
    "@loopx/core": "workspace:^",
    "@loopx/user-service": "workspace:^",
    "dotenv": "^16.3.1",
    "dotenv-extended": "^2.9.0",
    "log-server-urls": "^0.1.2"
  },
  "devDependencies": {
    "@nutol/build": "^0.2.14",
    "@nutol/eslint-config": "^0.2.9",
    "@nutol/ts-config": "^0.2.1",
    "@types/jest": "^29.5.3",
    "@types/node": "^20.4.10",
    "del-cli": "^5.0.0",
    "jest": "^29.6.2",
    "json": "^11.0.0",
    "replace": "^1.2.2",
    "tsx": "^3.12.7",
    "widdershins": "^4.0.1"
  },
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ]
}
